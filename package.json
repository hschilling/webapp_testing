{
    "name": "express-test-example",
    "version": "1.0.0",
    "description": "Example Express app with E2E tests",
    "main": "src/server.js",
    "scripts": {
      "start": "node src/server.js",
      "dev": "nodemon src/server.js",
      "test": "jest --detectOpenHandles",
      "test:e2e": "playwright test",
      "test:e2e:ui": "playwright test --ui"
    },
    "dependencies": {
      "express": "^4.18.2"
    },
    "devDependencies": {
      "@playwright/test": "^1.41.1",
      "jest": "^29.7.0",
      "nodemon": "^3.0.2",
      "supertest": "^6.3.3"
    }
  }